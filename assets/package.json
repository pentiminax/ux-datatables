{
    "name": "@pentiminax/ux-datatables",
    "description": "DataTables integration for Symfony",
    "license": "MIT",
    "version": "1.0.0",
    "type": "module",
    "main": "dist/controller.js",
    "symfony": {
        "controllers": {
            "datatable": {
                "main": "dist/controller.js",
                "webpackMode": "eager",
                "fetch": "eager",
                "enabled": true,
                "autoimport": {
                    "datatables.net-bs5/css/dataTables.bootstrap5.min.css": false,
                    "datatables.net-dt/css/dataTables.dataTables.min.css": true
                }
            }
        },
        "importmap": {
            "@hotwired/stimulus": "^3.0.0",
            "datatables.net-bs5": "^2.2.2",
            "datatables.net-buttons": "^3.2.2",
            "datatables.net-buttons-bs5": "^3.2.2",
            "datatables.net-buttons-dt": "^3.2.2",
            "datatables.net-buttons/js/buttons.html5": "^3.2.2",
            "datatables.net-buttons/js/buttons.print": "^3.2.2",
            "datatables.net-dt": "^2.2.2",
            "datatables.net-select-bs5": "^3.0.0",
            "datatables.net-select-dt": "^3.0.0",
            "jszip": "^3.10.1",
            "pdfmake": "^0.2.18",
            "pdfmake/build/vfs_fonts": "^0.2.18"
        }
    },
    "peerDependencies": {
        "@hotwired/stimulus": "^3.0.0",
        "datatables.net-bs5": "^2.2.2",
        "datatables.net-buttons": "^3.2.2",
        "datatables.net-buttons-bs5": "^3.2.2",
        "datatables.net-buttons-dt": "^3.2.2",
        "datatables.net-buttons/js/buttons.html5": "^3.2.2",
        "datatables.net-buttons/js/buttons.print": "^3.2.2",
        "datatables.net-dt": "^2.2.2",
        "datatables.net-select-bs5": "^3.0.0",
        "datatables.net-select-dt": "^3.0.0",
        "jszip": "^3.10.1",
        "pdfmake": "^0.2.18",
        "pdfmake/build/vfs_fonts": "^0.2.18"
    },
    "devDependencies": {
        "@hotwired/stimulus": "^3.0.0",
        "datatables.net-bs5": "^2.2.2",
        "datatables.net-buttons": "^3.2.2",
        "datatables.net-buttons-bs5": "^3.2.2",
